<div metal:define-macro="body">
	<br />
	<table><tr><td class="align-top">
	<div id="choices">

		<div tal:repeat="item dropdowns" id="dropdowndiv">
			${item['ddlabel']}
			<select id="${item['ddidentifier']}" onChange="dropdownCallback(this);" tal:attributes="disabled '' if item['disabled'] == True else None">
				<option value="--SELECT">--SELECT--</option>
				<option tal:repeat="pop item['dditems']" value="${pop['name']}" tal:attributes="selected '' if pop['selected'] == True else None">${pop['name']}</option>
			</select>
		</div>
	</div>
	</td>
	</tr>
		<tr>
			<td class="align-top">
				<div id="graph">Graph Goes Here</div>
			</td>
		</tr>
	</table>
</div>

