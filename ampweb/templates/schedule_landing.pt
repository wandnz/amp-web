<div id="content" metal:define-macro="body">

    <h2>Mesh Schedules</h2>
    <ul>
        <li tal:repeat="mesh meshes">
            <a href="/schedule/view/${mesh.name}">
                ${mesh.longname} (${mesh.name})
            </a>
        </li>
    </ul>

    <h2>Site Schedules</h2>
    <ul>
        <li tal:repeat="source sources">
            <a href="/schedule/view/${source.ampname}">
                ${source.longname} (${source.ampname})
            </a>
        </li>
    </ul>
</div>
