<div id="content" metal:define-macro="body">
    <div class="columnsContainer">
      <div class="leftColumn">
        <div id="eventspace">

          <div id="eventheader">
            <div class="eventheader-left">
                <h2>Recent event groups</h2>
            </div>
            <div class="eventheader-right">
                <button id="filterbut" type="button" class="btn btn-primary pull-right" data-target="#filterpanel" data-toggle="collapse" aria-expanded="false" aria-controls="filterpanel">Change Event Filters</button>
            </div>
          </div>

          <div class="filter-panel collapse" id="filterpanel">
            <div class="btn-group common-check" data-toggle="buttons" tal:condition="not:showcommon">
              <label class="btn btn-primary active" id="commonbutton">
                <input type="checkbox" autocomplete="off" checked>
                  Removing Common Events
              </label>
            </div>
            <div class="btn-group common-check" data-toggle="buttons" tal:condition="showcommon">
              <label class="btn btn-primary" id="commonbutton">
                <input type="checkbox" autocomplete="off">
                  Showing Common Events
              </label>
            </div>

            <div class="form-group">
              <form class="form-horizontal max-groups" role="form">
                <label for="maxgroups" class=" col-md-9 control-label">Max Event Groups:</label>
                <div class="col-md-3">
                <input type="text" class="form-control pull-right" id="maxgroups" value="${max_event_count}">
                </div>
              </form>
            </div>

            <div class="form-group filter-select">
              <form class="form-horizontal" role="form">
                <div class="col-md-6">
                  <select class="form-control" id="ASfiltername">
                  </select>
                </div>
                <div class="col-md-3">
                  <select class="form-control" id="ASfiltertype">
                    <option value='include'>Include</option>
                    <option value='exclude'>Exclude</option>
                    <option value='highlight'>Highlight</option>
                  </select>
                </div>
                <div class="col-md-3">
                  <button id="ASfilterbutton" type="button" class="btn btn-primary pull-right" onclick="updateASFilter();">Update AS Filter</button>
                </div>
              </form>
            </div>

            <div class="col-md-12" id="ASfiltershow">
              <label tal:condition="len(asincludes) > 0">Including only event groups involving:</label>
              <p class="form-control-static asfilter-list">
                <span class="filtered-name" tal:repeat="asx asincludes">
                  <span class="glyphicon glyphicon-remove" onclick="removeDashboardASFilter('${asx.number}', 'include');">
                  </span>
                  <span class="included-name">&nbsp;AS${asx.number} - ${asx.name}</span>
                </span>
              </p>
              <label tal:condition="len(asexcludes) > 0">Excluding event groups involving:</label>
              <p class="form-control-static asfilter-list">
                <span class="filtered-name" tal:repeat="asx asexcludes">
                  <span class="glyphicon glyphicon-remove" onclick="removeDashboardASFilter('${asx.number}', 'exclude');">
                  </span>
                  <span class="excluded-name">&nbsp;AS${asx.number} - ${asx.name}</span>
                </span>
              </p>
              <label tal:condition="len(ashighlights) > 0">Highlighting event groups involving:</label>
              <p class="form-control-static asfilter-list">
                <span class="filtered-name" tal:repeat="asx ashighlights">
                  <span class="glyphicon glyphicon-remove" onclick="removeDashboardASFilter('${asx.number}', 'highlight');">
                  </span>
                  <span class="highlighted-name">&nbsp;AS${asx.number} - ${asx.name}</span>
                </span>
              </p>
              
            </div>

            <div class="form-group filter-select">
              <form class="form-horizontal" role="form">
                <div class="col-md-6">
                  <select class="form-control" id="Srcfiltername" disabled>
                    <option>Loading Source list...</option>
                  </select>
                </div>
                <div class="col-md-3">
                  <select class="form-control" id="Srcfiltertype">
                    <option>Include</option>
                    <option>Exclude</option>
                    <option>Highlight</option>
                  </select>
                </div>
                <div class="col-md-3">
                  <button id="Srcfilterbutton" type="button" class="btn btn-primary pull-right">Update Source Filter</button>
                </div>
              </form>
            </div>
            
            <div class="col-md-12" id="Srcfiltershow">
              <label tal:condition="len(srcincludes) > 0">Including only events involving:</label>
              <p class="form-control-static asfilter-list">
                <span class="filtered-name" tal:repeat="src srcincludes">
                  <span class="glyphicon glyphicon-remove" onclick="removeDashboardSrcFilter('${src}', 'include');">
                  </span>
                  <span class="included-name">&nbsp;${src}</span>
                </span>
              </p>
              <label tal:condition="len(srcexcludes) > 0">Excluding events involving:</label>
              <p class="form-control-static asfilter-list">
                <span class="filtered-name" tal:repeat="src srcexcludes">
                  <span class="glyphicon glyphicon-remove" onclick="removeDashboardSrcFilter('${src}', 'exclude');">
                  </span>
                  <span class="excluded-name">&nbsp;${src}</span>
                </span>
              </p>
              <label tal:condition="len(srchighlights) > 0">Highlighting event groups containing:</label>
              <p class="form-control-static asfilter-list">
                <span class="filtered-name" tal:repeat="src srchighlights">
                  <span class="glyphicon glyphicon-remove" onclick="removeDashboardASFilter('${src}', 'highlight');">
                  </span>
                  <span class="highlighted-name">&nbsp;${src}</span>
                </span>
              </p>

            </div>

            <div class="form-group filter-select">
              <form class="form-horizontal" role="form">
                <div class="col-md-6">
                  <select class="form-control" id="Targetfiltername" disabled>
                    <option>Loading Target list...</option>
                  </select>
                </div>
                <div class="col-md-3">
                  <select class="form-control" id="Targetfiltertype">
                    <option>Include</option>
                    <option>Exclude</option>
                    <option>Highlight</option>
                  </select>
                </div>
                <div class="col-md-3">
                  <button id="Targetfilterbutton" type="button" class="btn btn-primary pull-right">Update Target Filter</button>
                </div>
              </form>
            </div>

            <div class="col-md-12" id="Destfiltershow">
              <label tal:condition="len(destincludes) > 0">Including only events involving:</label>
              <p class="form-control-static asfilter-list">
                <span class="filtered-name" tal:repeat="dest destincludes">
                  <span class="glyphicon glyphicon-remove" onclick="removeDashboardSrcFilter('${dest}', 'include');">
                  </span>
                  <span class="included-name">&nbsp;${dest}</span>
                </span>
              </p>
              <label tal:condition="len(destexcludes) > 0">Excluding events involving:</label>
              <p class="form-control-static asfilter-list">
                <span class="filtered-name" tal:repeat="dest destexcludes">
                  <span class="glyphicon glyphicon-remove" onclick="removeDashboardSrcFilter('${dest}', 'exclude');">
                  </span>
                  <span class="excluded-name">&nbsp;${dest}</span>
                </span>
              </p>
              <label tal:condition="len(desthighlights) > 0">Highlighting event groups containing:</label>
              <p class="form-control-static asfilter-list">
                <span class="filtered-name" tal:repeat="dest desthighlights">
                  <span class="glyphicon glyphicon-remove" onclick="removeDashboardASFilter('${dest}', 'highlight');">
                  </span>
                  <span class="highlighted-name">&nbsp;${dest}</span>
                </span>
              </p>

            </div>

            <div class="col-md-12 evtype-space text-center" id="EventTypeButtons">
              <label class="col-md-12"> Toggle Event Types</label>
              <button id="toggleLatencyIncr" type="button" onclick="toggleEventType('latency-incr');" tal:attributes="class latincr_attrs.classx; checked latincr_attrs.checked | nothing">
                <span class="glyphicon glyphicon-circle-arrow-up groupicon"></span>
              </button>

              <button id="toggleLatencyDecr" type="button" onclick="toggleEventType('latency-decr');" tal:attributes="class latdecr_attrs.classx; checked latdecr_attrs.checked | nothing">
                <span class="glyphicon glyphicon-circle-arrow-down groupicon"></span>
              </button>

              <button id="toggleRouteChange" type="button" onclick="toggleEventType('route-change');" tal:attributes="class routechange_attrs.classx; checked routechange_attrs.checked | nothing">
                <span class="glyphicon glyphicon-random groupicon"></span>
              </button>

            </div>




          </div>


          <div class="panel-group" id="recentevents" role="tablist" aria-multiselectable="true">
          </div>
          <p><a href="eventlist">+ see older event groups</a></p>
        </div>
      </div>
      <div class="rightColumn">
        <div id="graphspace">
          <div id="graphheader">
            <div class="graphheader-left">
                <h2>Event Statistics</h2>
            </div>
            <div class="graphheader-right">
            </div>
          </div>
    
          <div class="panel-group" id="dashgraphs" role="tablist" 
            aria-multiselectable="true">
            <div class="panel panel-default statspanel pull-left" id="tspanel" 
              data-toggle="collapse" data-target="#tsgraphpanel">

              <div id="recenteventts" class="panel-heading collapsed statspanel" 
                role="tab">
                <div class='panel-heading-text'>
                  <h2 class="panel-title">
                  <a> Recent Events:
                      ${total_event_count} events in
                      ${total_group_count} groups during the last day
                  </a>
                  </h2>
                </div>
                <div class='pull-right'>
                  <span class='glyphicon groupicon glyphicon-collapse-down'
                        id='tsicon'>
                  </span>
                </div>
              </div>

              <div id="tsgraphpanel" class="panel-collapse collapse"
                role="tabpanel" aria-labelledby="recenteventts">
                <div class="panel-body">
                    <div class="dashboardgraph" id="tsgraph"> </div>
                </div>
              </div>
            </div>
            <div class="panel panel-default pull-left statspanel" 
              id="topaspanel" data-toggle="collapse"
              data-target="#topasgraphpanel">

              <div id="topasheading" class="panel-heading statspanel collapsed" role="tab">
                <div class='panel-heading-text'>
                <h2 class="panel-title">
                   <a>
                   Top networks for events in the last day
                   </a>
                </h2>
                </div>
                <div class='pull-right'>
                  <span class='glyphicon groupicon glyphicon-collapse-down'
                        id='sourceicon'>
                  </span>
                </div>
              </div>

              <div id="topasgraphpanel" class="panel-collapse collapse"
                role="tabpanel" aria-labelledby="topasheading">
                <div class="panel-body">
                    <div class="dashboardgraph" id="source_graph"> </div>
                </div>
              </div>
            </div>
            <div class="panel panel-default pull-left statspanel" id="commonpanel" 
              data-toggle="collapse" data-target="#commongraphpanel">

              <div id="commonevents" class="panel-heading collapsed statspanel" role="tab">
                <div class='panel-heading-text'>
                <h2 class="panel-title">
                   <a>
                   Most common events in the past day.
                   </a>
                </h2>
                </div>
                <div class='pull-right'>
                  <span class='glyphicon groupicon glyphicon-collapse-down'
                        id='commonicon'>
                  </span>
                </div>
              </div>

              <div id="commongraphpanel" class="panel-collapse collapse"
                role="tabpanel" aria-labelledby="commonevents">
                <div class="panel-body">
                    <div class="dashboardgraph table-responsive">
                        <table class="table table-bordered" id="common_table">

                        </table>
                    </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>


  </div>
</div>
