<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title">Add new data series</h4>
        </div>

        <div metal:use-macro="modal_body" class="modal-body">
        </div>

        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">
                Discard changes
            </button>
            <button type="button" id="submit" class="btn btn-primary" onclick="graphPage.modal.submit();" disabled>Add data series</button>
        </div>
    </div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->
