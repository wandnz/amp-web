<div metal:define-macro="body">
    <h3>This is the matrix graph for: ${ipVersion} ${dataType} from ${src} to ${dst}</h3>
    <table id="AMP_matrix">
        <thead>
            <tr>
                <th class="top_left_node"></th>
                <th class="top_node" tal:repeat="destination dstList">${destination}</th>
            </tr>
        </thead>
        <tbody>
            <tr tal:repeat="source srcList">
                <td class="side_node">${source}</td>
                <td class="data" tal:repeat="destination dstList">Data!</td>
            </tr>
        </tbody>
    </table>
</div>
