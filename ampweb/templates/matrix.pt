<div metal:define-macro="body">
    <p>This is the matrix graph for: ${ipVersion} ${dataType} from ${src} to ${dst}</p>
    <table id="AMP_matrix">
        <thead>
            <tr>
                <th></th>
                <th tal:repeat="destination dstList">${destination}</th>
            </tr>
        </thead>
        <tbody>
            <tr tal:repeat="source srcList">
                <td>${source}</td>
                <td tal:repeat="destination dstList">Data!</td>
            </tr>
        </tbody>
    </table>
</div>
